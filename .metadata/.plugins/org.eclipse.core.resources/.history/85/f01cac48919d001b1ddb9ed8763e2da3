package Invoice;

import javax.swing.JOptionPane;

public class invoiceTeste {

	public static void main(String[] args) {
		
		Invoice fatura = new Invoice();
			fatura.setNumero("1");
			fatura.setDescricao("TESTE");
			fatura.setQuantidade(10);
			fatura.setPreco(50.0);
			
			
			String mensagem =String.format("Dados:%n Número:%s%n Descrição:%s%n Quantidade:%d%n Preço:%.2f%n Total:%.2f", fatura.getNumero(), fatura.getDescricao(), fatura.getQuantidade(), fatura.getPreco(), fatura.getInvoiceAmount());
			
			JOptionPane.showMessageDialog(null,mensagem);

	}

}
