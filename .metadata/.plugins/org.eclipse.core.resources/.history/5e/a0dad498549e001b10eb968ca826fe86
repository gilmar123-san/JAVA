public class Date {
	private int mes;
	private int dia;
	private int ano;
	
	public Date() {
		this.mes = 0;
		this.dia = 0;
		this.ano = 0;
	}

	public int getMes() {
		return mes;
	}

	public void setMes(int mes) {
		this.mes = mes;
	}

	public int getDia() {
		return dia;
	}

	public void setDia(int dia) {
		this.dia = dia;
	}

	public int getAno() {
		return ano;
	}

	public void setAno(int ano) {
		this.ano = ano;
	}
	
	public 
	
}
