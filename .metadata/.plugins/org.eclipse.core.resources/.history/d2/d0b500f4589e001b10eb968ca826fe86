import javax.swing.JOptionPane;

public class Date {
	private String mes;
	private String dia;
	private String ano;
	private String mensagem;
	
	public Date() {
		this.mes = "0";
		this.dia = "0";
		this.ano = "0";
	}

	public String getMes() {
		return mes;
	}

	public void setMes() {
		this.mes = JOptionPane.showInputDialog("Informe o mês:");
	}

	public String getDia() {
		return dia;
	}

	public void setDia() {
		this.dia = JOptionPane.showInputDialog("Informe o dia:");
	}

	public String getAno() {
		
		return ano;
	}

	public void setAno() {
		
		this.ano = JOptionPane.showInputDialog("Informe o ano:");
	}

	
}
